        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".examples.counter.CounterActivity"/>
        <activity android:name=".examples.counter.CounterActivity2"/>
    </application>

</manifest>